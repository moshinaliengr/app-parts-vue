{
  "name": "diesel-parts",
  "version": "1.0.5",
  "description": "A Diesel Laptops application",
  "productName": "Diesel Parts",
  "author": "mjackson <mjackson@diesellaptops.com>, jonathansloan <jonathanssloan@protonmail.com>",
  "private": true,
  "scripts": {
    "buildA": "quasar build -m capacitor -T android --ide",
    "buildI": "npm run iconsI && npm run splash && quasar build -m capacitor -T ios --ide",
    "devA": "quasar dev -m capacitor -T android",
    "devI": "quasar dev -m capacitor -T ios",
    "iconsA": "icongenie g -m capacitor --skip-trim -i app-icon.png -b app-splash.png",
    "iconsI": "icongenie g -m capacitor --skip-trim -i app-icon-ios.png",
    "rebuild": "quasar clean && npm run icons && quasar build mode spa",
    "splash": "icongenie g -f splashscreen --splashscreen-icon-ratio 30 -i app-icon.png -b app-splash.png",
    "signAPK": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore diesel-parts.keystore ./dist/capacitor/android/apk/release/app-release-unsigned.apk diesel-parts",
    "zipAlign": "zipalign -v 4 ./dist/capacitor/android/apk/release/app-release-unsigned.apk",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.15.4",
    "@quasar/cli": "^1.2.2",
    "@quasar/extras": "^1.11.3",
    "@quasar/icongenie": "^2.4.0",
    "analytics.js": "^2.9.1",
    "axios": "^0.21.4",
    "core-js": "^3.19.0",
    "dotenv": "^8.6.0",
    "flush-promises": "^1.0.2",
    "launchdarkly-js-client-sdk": "^2.20.0",
    "moment": "^2.29.1",
    "papaparse": "^5.3.1",
    "quasar": "^1.16.0",
    "remove-markdown": "^0.3.0",
    "swiper": "^7.2.0",
    "vue-showdown": "^2.4.1"
  },
  "devDependencies": {
    "@quasar/app": "^2.1.14",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-vue": "^6.1.2",
    "prettier": "2.1.2"
  },
  "browserslist": [
    "ie >= 11",
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
